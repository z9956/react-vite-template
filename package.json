{
	"name": "react-vite-template",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"build": "tsc && vite build",
		"dev": "vite",
		"format": "prettier --write .",
		"lint": "eslint --fix --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
